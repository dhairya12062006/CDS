To study and implement Operators in C++
#include
using namespace std;

int main() {
int a = 10, b = 5;

cout << "Addition: " << a + b << endl;
cout << "Subtraction: " << a - b << endl;
cout << "Multiplication: " << a * b << endl;
cout << "Division: " << a / b << endl;

cout << "Is a greater than b? " << (a > b) << endl;

cout << "Is a > 0 and b > 0? " << (a > 0 && b > 0) << endl;

return 0;
}
